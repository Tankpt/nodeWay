<%- include header %>

<% if (starVo.length>0) { %>
<ul>
    <% starVo.forEach(function(star){ %>
        <a href="https://github.com/<%= star.name %>" target="_blank"><%= star.name %></a><br>
    <% }) %>
</ul>
<% } %>

<%- include footer %>
<script>
    $(".j-delete").on("click",function(){
        var  _tmp =$(this).prev()[0];
        console.log(_tmp);
        $.ajax({
            type: "POST",
            url: "/deleteRss",
            data: { url: _tmp.dataset.id || '' }
        }).done(function( msg ) {
            if(msg=="true"){
                alert("删除成功");
                window.location.reload();
            }else{
                alert("删除失败");
            }
        });
    });
</script>
</html>